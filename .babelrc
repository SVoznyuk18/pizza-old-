{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.6.5"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "jsx-control-statements",
    [
      "babel-plugin-styled-components",
      {
        "displayName": true
      }
    ],
    "@babel/plugin-syntax-dynamic-import"
  ]
}